 as drinkAPI from '../../utilities/drink-api';\nimport './DrinkListPage.css';\nimport { Link } from 'react-router-dom';\nimport BrandList from '../../components/BrandList/BrandList';\nimport DrinkList from '../../components/DrinkList/DrinkList';\nimport DrinkDetail from '../../components/DrinkDetail/DrinkDetail';\nimport UserLogOut from '../../components/UserLogOut/UserLogOut';\n\n\nexport default function ({ user, setUser, drinkItems, setDrinkItems, activeBd, setActiveBd, brands }) {\n\n\n\n  const [drink, setDrink] = useState(null);\n\n\n  async function handleDetail(itemId) {\n    const drink = await drinkAPI.getById(itemId);\n    setDrink(drink);\n  }\n\n  return (\n    <main className=\"DrinkListPage\">\n      <aside>\n        <BrandList\n          brands={brands}\n          activeBd={activeBd}\n          setActiveBd={setActiveBd}\n        />\n        <Link to=\"/drinks/new\" className=\"button btn-sm\">New Drink</Link>\n        <UserLogOut user={user} setUser={setUser} />\n      </aside>\n      <DrinkList\n        drinkItems={drinkItems.filter(item => item.brand.name === activeBd)}\n        handleDetail={handleDetail}\n      />\n      <DrinkDetail drink={drink} />\n    </main>\n  );\n}","/Users/Coby/code/react-energy/src/utilities/drink-api.js",[],"/Users/Coby/code/react-energy/src/utilities/send-request.js",[],"/Users/Coby/code/react-energy/src/components/BrandList/BrandList.jsx",[],"/Users/Coby/code/react-energy/src/components/UserLogOut/UserLogOut.jsx",[],"/Users/Coby/code/react-energy/src/components/DrinkList/DrinkList.jsx",[],"/Users/Coby/code/react-energy/src/components/DrinkListItem/DrinkListItem.jsx",["87"],"import './DrinkListItem.css'; \n\nexport default function DrinkListItem({drinkItem, handleDetail}) {\n  return (\n    <div className=\"DrinkListItem\">\n      <div className=\"image\" ><img src={drinkItem.image}/></div>\n      <div className=\"name\">{drinkItem.name}</div>\n      <button onClick={() => handleDetail(drinkItem._id)}>Details</button>\n    </div>\n  );\n}","/Users/Coby/code/react-energy/src/components/DrinkDetail/DrinkDetail.jsx",[],"/Users/Coby/code/react-energy/src/pages/AddDrinkPage/AddDrinkPage.jsx",["88","89"],"import { useState, useRef, useEffect } from 'react';\nimport BrandListItems from '../../components/BrandListItems/BrandListItems';\n\n\n\n\nexport default function AddDrinkPage(props){\n  const [invalidForm, setValidForm] = useState(true);\n  const [formData, setFormData] = useState({\n    name: '',\n    brand: '',\n    image: '',\n    price: 0,\n  })\n\n  const formRef = useRef();\n\n  useEffect(() => {\n    formRef.current.checkValidity() ? setValidForm(false) : setValidForm(true);\n  });\n  useEffect(function() {\n    if (props.brands.length) setFormData({...formData, brand: props.brands[0]._id})\n  }, [props.brands])\n\n  const handleSubmit = (e) => {\n    e.preventDefault()\n    props.handleAddDrink(formData);\n  }\n\n\n  const handleChange = (e) => {\n    setFormData({\n      ...formData,\n      [e.target.name]: e.target.value\n    })\n  }\n\n  return (\n    <>\n      <h1>Add A Drink</h1>\n      <form  autoComplete=\"off\" ref={formRef} onSubmit={handleSubmit}>\n        <div className=\"form-group\">\n          <label>Drink Name (required)</label>\n          <input\n            className=\"form-control\"\n            name=\"name\"\n            value={formData.name}\n            onChange={handleChange}\n            required\n          />\n        </div>\n        <div className=\"form-group\">\n          <label>Drink Brand (required)</label>\n          <select \n            className=\"form-control\"\n            name=\"brand\"\n            value={formData.brand}\n            onChange={handleChange}\n            required\n            >\n           <BrandListItems brands={props.brands}/>\n\n          </select>\n        </div>\n        <div className=\"form-group\">\n          <label>Drink Image URL</label>\n          <input\n            className=\"form-control\"\n            name=\"image\"\n            value={ formData.image}\n            onChange={handleChange}\n            placeholder=\"https://m.media-amazon.com/images/I/71cCAJ+VhwL._AC_UL640_FMwebp_QL65_.jpg\"\n          />\n        </div>\n        <div className=\"form-group\">\n          <label>Drink Price</label>\n          <input\n            className=\"form-control\"\n            name=\"price\"\n            value={ formData.price}\n            onChange={handleChange}\n          />\n        </div>\n        <button\n          type=\"submit\"\n          className=\"btn\"\n          disabled={invalidForm}\n        >\n          ADD Drink\n        </button>\n      </form>\n    </>\n  );\n  \n}","/Users/Coby/code/react-energy/src/components/BrandListItems/BrandListItems.jsx",[],"/Users/Coby/code/react-energy/src/pages/DrinkDetailPage/DrinkDetailPage.jsx",[],"/Users/Coby/code/react-energy/src/components/DrinkCard/DrinkCard.jsx",[],{"ruleId":"90","replacedBy":"91"},{"ruleId":"92","replacedBy":"93"},{"ruleId":"94","severity":1,"message":"95","line":1,"column":20,"nodeType":"96","messageId":"97","endLine":1,"endColumn":29},{"ruleId":"94","severity":1,"message":"98","line":1,"column":31,"nodeType":"96","messageId":"97","endLine":1,"endColumn":37},{"ruleId":"99","severity":1,"message":"100","line":11,"column":1,"nodeType":"101","endLine":41,"endColumn":2},{"ruleId":"102","severity":1,"message":"103","line":6,"column":31,"nodeType":"104","endLine":6,"endColumn":59},{"ruleId":"105","severity":1,"message":"106","line":18,"column":3,"nodeType":"96","endLine":18,"endColumn":12,"suggestions":"107"},{"ruleId":"105","severity":1,"message":"108","line":23,"column":6,"nodeType":"109","endLine":23,"endColumn":20,"suggestions":"110"},"no-native-reassign",["111"],"no-negated-in-lhs",["112"],"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","'useRef' is defined but never used.","import/no-anonymous-default-export","Unexpected default export of anonymous function","ExportDefau